  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                id,\n                subscription_intent,\n                signature,\n                subscription_id,\n                subscriber,\n                merchant,\n                amount,\n                interval_seconds,\n                start_time,\n                max_payments,\n                max_total_amount,\n                expiry,\n                nonce,\n                processed,\n                created_at,\n                processed_at,\n                chain,\n                avail_block_number,\n                avail_extrinsic_index\n            FROM intent_cache\n            WHERE processed = false\n                AND chain = $1\n                AND expiry > NOW()\n            ORDER BY created_at ASC\n            LIMIT $2\n            ",
